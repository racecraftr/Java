import java.util.Scanner;
public class Pp3 {
	public static void main(String [] args) {
		Scanner scn = new Scanner(System.in);
		System.out.println("Enter a degree of farenheight and I will convert it to a degree of celcius.");
		float f = scn.nextFloat();
		float c = (f-32)*5/9;
		System.out.println(f + "ยบ farenheigt is equal to \n" + c + "ยบ celcius.");
	}

}
